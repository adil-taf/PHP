{
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "psr/container": "^2.0",
    "symfony/mailer": "^7.0",
    "doctrine/dbal": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
